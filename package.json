{
  "scripts": {
    "deploy": "echo \"Use bun run deploy-dev or bun run deploy-prod instead\"",
    "deploy-dev": "bun run predeploy && bunx serverless deploy && bun run postdeploy",
    "deploy-prod": "cross-env STAGE=prod bun run deploy-dev",
    "predeploy": "bunx tsc lib/env.ts lib/config.ts",
    "postdeploy": "bunx rimraf lib/env.js lib/config.js",
    "lint": "bunx tsc --strict --noEmit && bunx eslint \"**/*.ts\"",
    "test": "bunx jest"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "dotenv": "^16.6.1",
    "email-addresses": "^5.0.0",
    "fast-deep-equal": "^3.1.3",
    "js-base64": "^3.7.8",
    "mailparser": "^3.9.0",
    "nodemailer": "^6.10.1",
    "random-words": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@types/aws-lambda": "^8.10.158",
    "@types/jest": "^29.5.14",
    "@types/js-base64": "^3.3.1",
    "@types/node": "^18.19.130",
    "@types/nodemailer": "^6.4.21",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "aws-sdk-mock": "^5.9.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eml-format": "^0.6.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "jest": "^29.7.0",
    "prettier": "^2.8.8",
